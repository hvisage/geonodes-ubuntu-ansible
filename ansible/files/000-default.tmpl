<VirtualHost *:80>
        ServerName {{ HOST_URL }}

        ServerAdmin webmaster@localhost
        DocumentRoot /var/www/html

        #LogLevel info ssl:warn

        ErrorLog ${APACHE_LOG_DIR}/error.log
        CustomLog ${APACHE_LOG_DIR}/access.log combined

        Alias /static /home/data/geonode/geonode/static_root/
        <Directory /home/data/geonode/geonode/static_root>
           Require all granted
        </Directory>

        <Directory /home/data/geonode/geonode>
           <Files wsgi.py>
               Require all granted
           </Files>
        </Directory>

        WSGIDaemonProcess geonode-wsgi python-home=/home/data/venv python-path=/home/data/geonode
        WSGIProcessGroup geonode-wsgi
        WSGIScriptAlias / /home/data/geonode/geonode/wsgi.py

        # ErrorLog ${APACHE_LOG_DIR}/qgis-server-error.log
        # CustomLog ${APACHE_LOG_DIR}/qgis-server-access.log combined

        FcgidIOTimeout 120
        FcgidInitialEnv LC_ALL "en_US.UTF-8"
        FcgidInitialEnv PYTHONIOENCODING UTF-8
        FcgidInitialEnv LANG "en_US.UTF-8"
        FcgidInitialEnv QGIS_DEBUG 1
        FcgidInitialEnv QGIS_SERVER_LOG_FILE /var/log/qgis-server.log-000.log
        FcgidInitialEnv QGIS_SERVER_LOG_LEVEL 0
        FcgidInitialEnv QGIS_PLUGINPATH "/opt/qgis-server/plugins"

        ScriptAlias /cgi-bin/ /usr/lib/cgi-bin/
        <Directory "/usr/lib/cgi-bin">
           AllowOverride All
           Options +ExecCGI -MultiViews +FollowSymLinks
           Require all granted
        </Directory>

        #Include conf-available/serve-cgi-bin.conf
</VirtualHost>
